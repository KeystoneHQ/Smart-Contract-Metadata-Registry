 fees accumulated."}},"version":1}},"settings":{"compilationTarget":{"browser/Kashi.sol":"KashiPairMediumRiskV1"},"evmVersion":"istanbul","libraries":{},"metadata":{"bytecodeHash":"ipfs"},"optimizer":{"enabled":true,"runs":800},"remappings":[]},"sources":{"browser/Kashi.sol":{"keccak256":"0x4ecbaa9a836edcbdbd80304acf4eb17eb362c06bc062b2876771cc7a2cd0883d","license":"UNLICENSED","urls":["bzz-raw://c0d77fe94c2f8e6c774d8c0e3cbb63771742f573201a2c163060314bb4b8ff2c","dweb:/ipfs/QmdtigTqtmYRYwvJwgkBu22pfWhsBCyJGiBVEbm3J5UUFN"]}},"version":1},"version":1,"checkPoints":[]}